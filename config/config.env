# ----------------------------
# Flywheel-Lite defaults
# ----------------------------

# Interview behavior
FW_INTERVIEW_ROUNDS=2

# Plan refinement loop behavior (GPT-5.2 loop count)
FW_PLAN_ITERS=3

# Default worker mix for startwork
FW_CC=3
FW_COD=3
FW_GMI=1

# Claude model alias - sonnet for faster bead execution
FW_CLAUDE_MODEL=sonnet

# Codex models
FW_CODEX_IMPL_MODEL=gpt-5.3-codex
FW_GPT_PLAN_MODEL=gpt-5.2
FW_CODEX_REASONING=xhigh

# Gemini model
FW_GEMINI_MODEL=gemini-2.5-pro

# If set to 1, prefer safer modes (less autonomy)
FW_SAFE=0
